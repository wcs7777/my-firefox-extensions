{
  "manifest_version": 2,
  "name": "Save Links",
  "version": "1.2.0",
  "description": "Save selected links to download or open them all later",
  "icons": {
    "48": "icons/icon.png"
  },
  "options_ui": {
    "page": "src/options/options.html"
  },
  "background": {
    "page": "src/background/background.html",
    "persistent": true
  },
  "permissions": [
    "storage",
    "menus",
		"tabs",
    "activeTab",
		"downloads"
  ]
}
