{
  "manifest_version": 2,
  "name": "Successor Tab Id",
  "version": "1.0.3",
  "description": "Return to the previous activated tab when a tab is closed",
  "icons": {
    "48": "icons/icon.png"
  },
  "options_ui": {
    "page": "src/options.html"
  },
  "background": {
    "page": "src/background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/icon-19.png",
      "38": "icons/icon-38.png"
    },
    "default_title": "Return Tab When Closing"
  },
  "permissions": [
    "storage"
  ]
}
