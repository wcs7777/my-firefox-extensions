{
  "manifest_version": 2,
  "name": "Search",
  "version": "1.0.10",
  "description": "Context Menu for search selected text in defined websites",
  "icons": {
    "48": "icons/icon.png"
  },
  "background": {
    "page": "src/background.html",
    "persistent": true
  },
  "options_ui": {
    "page": "src/options.html"
  },
  "permissions": [
    "menus",
    "storage"
  ]
}
